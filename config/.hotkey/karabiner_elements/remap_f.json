{
    "title": "^num を fnumにリマップ",
    "rules": [
        {
            "description": "ctrl + num を fn + numに変換する。例えばf7はctrl + 7になる",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "7",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f7",
                            "modifiers": []
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "6",
                        "modifiers": {
                            "mandatory": [
                                "left_control"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "f6",
                            "modifiers": []
                        }
                    ]
                }
            ]
        }
    ]
}